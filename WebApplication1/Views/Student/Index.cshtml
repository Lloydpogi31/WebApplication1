@{
    int num1 = 20;
    int num2 = 20;
    int total = num1 + num2;
    int product = num1 * num2;
    int diff = num1 - num2;
    int quo = num1 / num2;
    int mod = num1 % num2;

    var name = "Lloyd Tano";
    DateTime BirthDay = DateTime.Parse("31/01/2002");
    //DateTime Today = DateTime.Now;
    var gender = "Male";
    var age = DateTime.Now.Subtract(BirthDay).Days /365;

}

   <h2>Welcome Student View<br /></h2>

   <h2>I am @name @age Gender: @gender</h2>
 

   <table class="table table-dark">
       <tr>
           <td>Sum</td>
           <td>@total</td>
       </tr>

       <tr>
           <td>Minus</td>
           <td>@diff</td>
       </tr>

       <tr>
           <td>Quotient</td>
